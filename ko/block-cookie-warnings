<script src="/js/vendor/bowser.js?934067182"></script>

<script>

addEventListener('load', function() {
  var imgEl;

  var browserNames = {
    'Chrome': 'chrome',
    'Firefox': 'firefox',
    'Internet Explorer': 'ie',
    'Microsoft Edge': 'edge',
    'Opera': 'opera',
    'Yandex Browser': 'yandex'
  };

  var browserName = browserNames[bowser.name];

  if ((window.location.href.indexOf('utm_source=abp') !== -1) || !browserName) {
    document.body.classList.add('abp-installed');

  } else {
    imgEl = document.querySelector('.download-abp .button-image');

    if (navigator.userAgent.indexOf("Edg/") !== -1) {
        browserName = 'edge';
    }
    imgEl.src = imgEl.src.split('/img/')[0] +
    '/img/dl-button-' + browserName + '.svg';

    imgEl.removeAttribute('hidden');
  }

  if (!(Object.values(browserNames).includes(browserName) && !bowser.mobile)) {
      document.getElementsByClassName('error-msg')[0].classList.add('visible');
      document.getElementsByClassName('subscribe-idcac')[0].classList.add('hide');
      document.getElementsByClassName('download-abp')[0].classList.add('hide');
      document.getElementsByClassName('note')[0].classList.add('hide');
  }

  function alignHeadings() {
    var howToAdd = document.querySelector('.how-to-add h2');
    var howToRemove = document.querySelector('.how-to-remove h2');

    if (window.innerWidth < 1366) {
      howToAdd.style.height = 'auto';
      howToRemove.style.height = 'auto';

    } else {

      if (howToAdd.clientHeight > howToRemove.clientHeight) {
        howToRemove.style.height = howToAdd.clientHeight + 'px';

      } else if (howToAdd.clientHeight < howToRemove.clientHeight) {
        howToAdd.style.height = howToRemove.clientHeight + 'px';
      }
    }
  }

  alignHeadings();

  addEventListener('resize', alignHeadings);
});

</script>

    

    

