          <ul>
            
              <li>
                <a href="#features">Nouvelles fonctionnalités</a>
                
              </li>
            
              <li>
                <a href="#interface">Interface utilisateur</a>
                
              </li>
            
              <li>
                <a href="#core">Noyau</a>
                
              </li>
            
